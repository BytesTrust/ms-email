[  {    "type": "post",    "url": "/emails/confirm-email/send",    "title": "Send a confirmation email",    "version": "1.0.0",    "name": "confirmEmail",    "group": "Emails",    "permission": [      {        "name": "none"      }    ],    "parameter": {      "fields": {        "Body Parameters": [          {            "group": "Body Parameters",            "type": "string",            "optional": false,            "field": "email",            "description": "<p>The email from target user</p>"          },          {            "group": "Body Parameters",            "type": "string",            "optional": false,            "field": "confirmationUrl",            "description": "<p>The link to confirm the email</p>"          }        ]      },      "examples": [        {          "title": "Example Request",          "content": "{\n     \"email\": \"james@smith.com\",\n     \"confirmationUrl\": \"https://www.google.com/confirm-email?token\",\n}",          "type": "json"        }      ]    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "string",            "optional": false,            "field": "message",            "description": "<p>A success message</p>"          },          {            "group": "Success 200",            "type": "object",            "optional": false,            "field": "data",            "description": "<p>The account created</p>"          }        ]      }    },    "filename": "src/routes/emails.route.js",    "groupTitle": "Emails",    "error": {      "fields": {        "Error 4xx": [          {            "group": "Error 4xx",            "type": "UnprocessableEntity",            "optional": false,            "field": "ValidationError",            "description": "<p>Validation Failed</p>"          },          {            "group": "Error 4xx",            "type": "NotFound",            "optional": false,            "field": "ContentNotFoundError",            "description": "<p>Content not found</p>"          },          {            "group": "Error 4xx",            "type": "NotFound",            "optional": false,            "field": "LanguageNotFoundError",            "description": "<p>Language not found</p>"          },          {            "group": "Error 4xx",            "type": "UnauthorizedError",            "optional": false,            "field": "MissingAuthorizationError",            "description": "<p>Missing authorization header</p>"          },          {            "group": "Error 4xx",            "type": "UnauthorizedError",            "optional": false,            "field": "MissingBearerError",            "description": "<p>Missing bearer</p>"          },          {            "group": "Error 4xx",            "type": "UnauthorizedError",            "optional": false,            "field": "InvalidTokenError",            "description": "<p>Invalid token</p>"          }        ]      },      "examples": [        {          "title": "ValidationError",          "content": "HTTP/1.1 422 Unprocessable Entity\n{\n    \"error\": {\n        \"status\": 422,\n        \"type\": \"UnprocessableEntity\",\n        \"name\": \"ValidationError\",\n        \"message\": \"Validation Failed\",\n        \"errors\": [{\n            \"field\": \"email\",\n            \"message\": \"This is a required field\"\n        }]\n    }\n}",          "type": "json"        },        {          "title": "ContentNotFoundError",          "content": "HTTP/1.1 404 Not Found\n{\n    \"error\": {\n        \"status\": 422,\n        \"type\": \"UnprocessableEntity\",\n        \"name\": \"ContentNotFoundError\",\n        \"message\": \"Content not found\",\n        \"errors\": []\n    }\n}",          "type": "json"        },        {          "title": "LanguageNotFoundError",          "content": "HTTP/1.1 404 Not Found\n{\n    \"error\": {\n        \"status\": 422,\n        \"type\": \"UnprocessableEntity\",\n        \"name\": \"LanguageNotFoundError\",\n        \"message\": \"Language not found\",\n        \"errors\": []\n    }\n}",          "type": "json"        },        {          "title": "MissingAuthorizationError",          "content": "HTTP/1.1 401 Unauthorized\n{\n    \"error\": {\n        \"status\": 401,\n        \"type\": \"UnauthorizedError\",\n        \"name\": \"MissingAuthorizationError\",\n        \"message\": \"Missing authorization header\",\n        \"errors\": []\n    }\n}",          "type": "json"        },        {          "title": "MissingBearerError",          "content": "HTTP/1.1 401 Unauthorized\n{\n    \"error\": {\n        \"status\": 401,\n        \"type\": \"UnauthorizedError\",\n        \"name\": \"MissingBearerError\",\n        \"message\": \"Missing bearer\",\n        \"errors\": []\n    }\n}",          "type": "json"        },        {          "title": "InvalidTokenError",          "content": "HTTP/1.1 401 Unauthorized\n{\n    \"error\": {\n        \"status\": 401,\n        \"type\": \"UnauthorizedError\",\n        \"name\": \"InvalidTokenError\",\n        \"message\": \"Invalid token\",\n        \"errors\": []\n    }\n}",          "type": "json"        }      ]    }  }]
